{
  "manifest_version": 2,

  "name": "Anti-Circumvention: Vanilla WebRequests",
  "description": "This extension is used to collect webrequests (vanilla)",
  "version": "0.0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentscript.js"],
		  "run_at": "document_start"
       
    }
  ],
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "background": {
    "scripts": ["app/background.js"],
    "persistent": true
  }, 
  "permissions": [
    "webRequest",
    "management",
    "notifications",
    "webNavigation",
    "downloads",
    "tabs",
    "debugger",
    "webRequestBlocking",
    "<all_urls>",
    "activeTab"
  ]
}